#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/psego/Development/OpenSource/emberjs/ember-performance/node_modules/.pnpm/qunit@2.21.1/node_modules/qunit/bin/node_modules:/Users/psego/Development/OpenSource/emberjs/ember-performance/node_modules/.pnpm/qunit@2.21.1/node_modules/qunit/node_modules:/Users/psego/Development/OpenSource/emberjs/ember-performance/node_modules/.pnpm/qunit@2.21.1/node_modules:/Users/psego/Development/OpenSource/emberjs/ember-performance/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/psego/Development/OpenSource/emberjs/ember-performance/node_modules/.pnpm/qunit@2.21.1/node_modules/qunit/bin/node_modules:/Users/psego/Development/OpenSource/emberjs/ember-performance/node_modules/.pnpm/qunit@2.21.1/node_modules/qunit/node_modules:/Users/psego/Development/OpenSource/emberjs/ember-performance/node_modules/.pnpm/qunit@2.21.1/node_modules:/Users/psego/Development/OpenSource/emberjs/ember-performance/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../node_modules/.pnpm/qunit@2.21.1/node_modules/qunit/bin/qunit.js" "$@"
else
  exec node  "$basedir/../../../node_modules/.pnpm/qunit@2.21.1/node_modules/qunit/bin/qunit.js" "$@"
fi
