    "_lint:hbs": "ember-template-lint .",
    "_lint:hbs:fix": "ember-template-lint . --fix",
    "_lint:js": "ember build && eslint . --cache",
    "_lint:js:fix": "ember build && eslint . --fix",
    "start": "vite",
    "_syncPnpm": "pnpm sync-dependencies-meta-injected",
